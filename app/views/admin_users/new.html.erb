<% @page_title = "New Admin User" %>

<%= link_to("<< Back to admin user menu", admin_users_path, :class => 'btn btn-link') %>

<div class="admin_users new">
	<h2 class="title">Create Admin User</h2>



	<%= form_for(@admin_user) do |f| %>
	
		<%= render(:partial => "form", :locals => {:f => f}) %>

		<div class="form-buttons btn">
			<%= f.submit("Create Admin User") %>
		</div>
	<% end %>
</div>
